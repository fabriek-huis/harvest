    "react-perfect-scrollbar": "^0.2.2",
    "react-popover": "^0.4.16",
    "react-redux": "^5.0.5",
    "react-responsive": "^1.3.4",
    "react-router": "^4.1.1",
    "react-router-dom": "^4.1.1",
    "react-router-redux": "next",
    "react-svg-inline": "^2.0.0",
    "react-toastify": "^2.0.0-rc.3",
    "rebass": "^1.0.0-8",
    "recompose": "^0.25.0",
